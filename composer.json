{
    "name": "max-s-lab/filesystem",
    "description": "A package for convenient work with local filesystem.",
    "keywords": [
        "filesystem",
        "filesystems",
        "file",
        "files",
        "directory",
        "directories"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Maksim Sprikov",
            "email": "spirkov.2001@mail.ru"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.3",
        "ext-fileinfo": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.6.0"
    },
    "autoload": {
        "psr-4": {
            "MaxSLab\\Filesystem\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MaxSLab\\Filesystem\\Tests\\": "tests/"
        }
    }
}